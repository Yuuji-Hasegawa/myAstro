{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm icon && pnpm image && astro dev",
    "start": "pnpm icon && pnpm image && astro dev",
    "build": "astro build && pnpm fix",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:js": "eslint \"./src/**/*.{js,jsx,astro}\"",
    "lint:style": "stylelint \"./src/**/*.{css,scss,astro}\"",
    "lint:prettier": "prettier --check src",
    "fix": "run-p fix:*",
    "fix:js": "npm run lint:js -- --fix",
    "fix:prettier": "prettier --write src",
    "fix:style": "npm run lint:style -- --fix",
    "icon": "run-s icon:clean icon:gen",
    "icon:gen": "node imagemin-script/icon.mjs",
    "icon:clean": "rimraf public/icons/icon-*.png",
    "image": "run-s image:clean image:opt image:gen",
    "image:opt": "node imagemin-script/opt.mjs",
    "image:gen": "node imagemin-script/gen.mjs",
    "image:clean": "rimraf src/public/img"
  },
  "dependencies": {
    "astro": "^2.0.17",
    "astro-compress": "^1.1.35",
    "autoprefixer": "^10.4.13",
    "css-declaration-sorter": "^6.3.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs": "0.0.1-security",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "rimraf": "^4.2.0",
    "sass": "^1.58.3",
    "sharp": "^0.31.3",
    "stylelint": "^15.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.4.0"
  }
}
